<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="0dp"
                android:background="@drawable/button">
    <TextView
            android:id="@+id/name_entry_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/texto_de_pruebas"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/inquiry_entry_icon"
            android:layout_margin="0dp"/>
    <ImageView
            android:id="@+id/inquiry_entry_icon"
            style="@style/CustomTheme.ListItem.ImageView.Left"
            android:src="@drawable/ic_launcher"/>

    <TextView android:id="@+id/notificationText"
              style="@style/CustomTheme.NotificationText" />
</RelativeLayout>